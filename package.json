{
  "name": "link-rel",
  "version": "1.0.0",
  "description": "List of link types for `rel` on `link`",
  "license": "MIT",
  "keywords": [
    "html",
    "link",
    "type",
    "rel",
    "attribute",
    "w3c"
  ],
  "repository": "https://github.com/wooorm/link-rel",
  "bugs": "https://github.com/wooorm/link-rel/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "main": "index.json",
  "files": [
    "index.json"
  ],
  "dependencies": {},
  "devDependencies": {
    "bail": "^1.0.0",
    "browserify": "^14.1.0",
    "concat-stream": "^1.5.2",
    "esmangle": "^1.0.1",
    "remark-cli": "^2.1.0",
    "remark-preset-wooorm": "^2.0.0",
    "tape": "^4.4.0",
    "xo": "^0.17.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail --output",
    "build-crawl": "node build",
    "build-bundle": "browserify index.json --bare -s linkRel > link-rel.js",
    "build-mangle": "esmangle link-rel.js > link-rel.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test.js",
    "test": "npm run build && npm run lint && npm run test-api"
  },
  "xo": {
    "space": true,
    "ignores": [
      "link-rel.js"
    ]
  },
  "remarkConfig": {
    "presets": "wooorm"
  }
}
